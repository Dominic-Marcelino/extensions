{
	"name": "ai-researcher-bundle ",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-bundle"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "bundle",
		"path": {
			"app": "dist/app.js",
			"api": "dist/api.js"
		},
		"entries": [
			{
				"type": "interface",
				"name": "ai-researcher-interface",
				"source": "src/ai-researcher-interface/index.js"
			},
			{
				"type": "endpoint",
				"name": "ai-researcher-endpoint",
				"source": "src/ai-researcher-endpoint/index.js"
			}
		],
		"host": "^10.10.0"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"add": "directus-extension add"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "11.0.3",
		"vue": "^3.4.26"
	},
	"dependencies": {
		"axios": "^1.6.8",
		"openai": "^4.44.0",
		"sass": "^1.77.0"
	}
}
